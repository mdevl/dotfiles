! General Settings
URxvt.termName: rxvt-unicode
URxvt.geometry: 134x33+240x380
URxvt.scrollBar: false
URxvt.internalBorder: 14
URxvt.iso14755: false

! Colors
#include "/home/blackcat/.colors/xoria"
 
! Xft
Xft.antialias:  1
Xft.autohint:   0
Xft.dpi:    96
Xft.hinting:    1
Xft.hintstyle:  hintslight
Xft.lcdfilter:  lcddefault
Xft.rgba: rgb

! Extention --
URxvt.perl-lib: /home/blackcat/.urxvt/urxvt-perls
URxvt.perl-ext-common:  default,clipboard,keyboard-select,url-select
URxvt.clipboard.autocopy: true
 
! Keybind
URxvt.keysym.Control-Alt-C: perl:clipboard:copy
URxvt.keysym.Control-Alt-V: perl:clipboard:paste
URxvt.keysym.M-Escape: perl:keyboard-select:activate
URxvt.keysym.C-Escape: perl:keyboard-select:activate
URxvt.keysym.C-u: perl:url-select:select_next

! Fonts

!Droid Sans Mono
!URxvt.font: xft:Droid Sans Mono-10
!URxvt.letterSpace: -1

!DejaVu Sans
!URxvt*font: xft:DejaVu Sans Mono-10
!URxvt.letterSpace: -1

!Monaco
!URxvt.font: xft:Monaco-10
!URxvt.letterSpace: -1

!Drift
!URxvt*font: -artwiz-drift-medium-r-normal--10-100-75-75-n-60-iso8859-1
!URxvt*boldFont: -artwiz-drift-medium-r-normal--10-100-75-75-n-60-iso8859-1
!URxvt*italicFont: -artwiz-drift-medium-r-normal--10-100-75-75-n-60-iso8859-1
!URxvt*bolditalicFont: -artwiz-drift-medium-r-normal--10-100-75-75-n-60-iso8859-1

!Metis
URxvt*font: -kori-metis-normal-r-normal--11-90-75-75-p-58-iso10646-1
URxvt*boldFont: -kori-metis-normal-r-normal--11-90-75-75-p-58-iso10646-1
URxvt*italicFont: -kori-metis-normal-r-normal--11-90-75-75-p-58-iso10646-1
URxvt*bolditalicFont: -kori-metis-normal-r-normal--11-90-75-75-p-58-iso10646-1

!Profont
!URxvt*font: -nil-profont-medium-r-normal--11-110-72-72-c-60-iso8859-1
!URxvt*boldFont: -nil-profont-medium-r-normal--11-110-72-72-c-60-iso8859-1
!URxvt*italicFont: -nil-profont-medium-r-normal--11-110-72-72-c-60-iso8859-1
!URxvt*bolditalicFont: -nil-profont-medium-r-normal--11-110-72-72-c-60-iso8859-1

